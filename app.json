{
  "expo": {
    "name": "guardian-bot",
    "slug": "guardian-bot",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "domains": ["*"]
    },
    "android": {
      "domains": ["*"]
    },
    "web": {
      
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow GuardianBot to use your location."
        }
      ]
    ]
  }
}
